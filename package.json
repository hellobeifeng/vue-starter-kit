{
  "name": "vue-starter-kit",
  "version": "1.0.0",
  "description": "一个用于快速搭建前端vue工程的脚手架",
  "main": "index.js",
  "scripts": {
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.config.client.js",
    "build": "npm run clean && npm run build:client",
    "clean": "rimraf dist",
    "lint": "eslint --ext .js --ext .vue src/",
    "lint-fix": "eslint --fix --ext .js --ext .vue src/",
    "precommit": "npm run lint-fix",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hellobeifeng/vue-starter-kit.git"
  },
  "author": "feng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hellobeifeng/vue-starter-kit/issues"
  },
  "homepage": "https://github.com/hellobeifeng/vue-starter-kit#readme",
  "devDependencies": {
    "autoprefixer": "7.2.3",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "1.6.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.7",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "2.30.1",
    "husky": "^0.14.3",
    "postcss-loader": "2.0.9",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "vue-loader": "13.6.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "2.5.13",
    "vuex": "^3.0.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "vue": "2.5.13"
  }
}
